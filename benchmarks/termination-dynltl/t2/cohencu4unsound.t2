START: 1;

FROM: 1;
      a := nondet();
      k := nondet();
      n := 0;
      x := 0;
      y := 1;
      z := 6;
      c := 0;
      
      p := 2;
      assume(y*z - 18*x - 12*y + 2*z - 6 + c <= k);
TO: 2;

FROM: 2;
      assume(y*z - 18*x - 12*y + 2*z - 6 + c <= k);
      p := 1;
TO: 3;

FROM: 3;
       n := n + 1;
       x := x + y;
       y := y + z;
       z := z + 6;
       c := c + 1;

TO: 2;
 
FROM: 2;
      assume(y*z - 18*x - 12*y + 2*z - 6 + c > k);
      p := c-k;
TO: 4;
