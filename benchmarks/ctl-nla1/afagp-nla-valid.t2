START: 0;                      

FROM: 0;
      k := nondet();
      x := 0;
      y := 0;
      c := 0;
      assume(c + (y * y) - 2 * x + y < k);
      p := 0;
TO: 1;

FROM: 1;
  assume(c + (y * y) - 2 * x + y < k);
TO: 2;

FROM: 2;
      c := c+1;
      y := y+1;
      x := y+x;
      p := 0;
TO: 1;

FROM: 1;
      assume(c + (y * y) - 2 * x + y >= k);
TO: 3; 

FROM: 3;
      nd := nondet();
TO: 4;

FROM: 4;
      assume(nd>0);
      p := 1;
TO: 5;

From: 5
      assume(nd<=0);
      p := 2;
TO: 3
