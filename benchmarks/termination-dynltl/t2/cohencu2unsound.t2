START: 1;

FROM: 1;
      a := nondet();
      k := nondet();
      n := 0;
      x := 0;
      y := 1;
      z := 6;
      
      p := 2;
      assume( 3 * n * n + 3 * n + 1 <= k);
TO: 2;

FROM: 2;
      assume( 3 * n * n + 3 * n + 1 <= k);
      p := 1;
TO: 3;

FROM: 3;
       n := n + 1;
       x := x + y;
       y := y + z;
       z := z + 6;

TO: 2;
 
FROM: 2;
      assume( 3 * n * n + 3 * n + 1 > k);
      p := y-k;
TO: 4;
